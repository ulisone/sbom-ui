# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore storage (uploaded files in development and test).
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/credentials/*.key

# Ignore precompiled assets
/public/assets
/public/packs
/public/packs-test

# Ignore node_modules
/node_modules
/yarn-error.log
.yarn-integrity

# Ignore uploaded files in development.
/public/uploads

# Ignore coverage reports
/coverage/*

# Ignore byebug history
.byebug_history

# Ignore environment variables
.env
.env.local
.env.*.local
.env.development.local
.env.test.local
.env.production.local

# Ignore database configuration (uncomment if not using version control for database.yml)
# /config/database.yml

# Ignore IDE/editor files
.idea/
.vscode/
*.swp
*.swo
*~
.project
.settings/

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore spring files
/spring/*.pid

# Ignore bootsnap cache
/tmp/cache/bootsnap*

# Ignore vendor bundle
/vendor/bundle

# Ignore Webpacker/Shakapacker
/public/packs
/public/packs-test
/node_modules

# Ignore Yarn
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/sdks
!.yarn/versions

# Ignore test artifacts
/spec/examples.txt
/test/reports/

# Ignore Redis dump
dump.rdb
